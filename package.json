{
  "name": "blog-express",
  "version": "1.0.0",
  "description": "Blog backend API created with expressjs",
  "main": "dist .",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "prebuild": "eslint . --ext .ts --fix",
    "seed": "ts-node src/database/seeder/index.ts",
    "build": "tsc",
    "prestart": "npm run build",
    "dev": "nodemon",
    "start": "ts-node src/index.ts",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pretest": "npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:hifisaputra/blog-express.git"
  },
  "author": "Hifi Saputra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hifisaputra/blog-express/issues"
  },
  "homepage": "https://github.com/hifisaputra/blog-express#readme",
  "_moduleAliases": {
    "@src": "dist"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose-delete": "^0.5.8",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.7.5",
    "mongoose-delete": "^0.5.4",
    "winston": "^3.5.1"
  }
}
